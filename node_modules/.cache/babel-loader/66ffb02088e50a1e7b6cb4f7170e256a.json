{"ast":null,"code":"import { Form, Field, ErrorMessage } from 'vee-validate';\nimport * as yup from 'yup';\nimport AuthService from '@/services/AuthService.js';\nexport default {\n  name: 'LoginView',\n  components: {\n    Form: Form,\n    Field: Field,\n    ErrorMessage: ErrorMessage\n  },\n  data: function data() {\n    var schema = yup.object().shape({\n      username: yup.string().required('User name is required!'),\n      password: yup.string().required('Password is required!')\n    });\n    return {\n      loading: false,\n      message: '',\n      schema: schema\n    };\n  },\n  methods: {\n    handleLogin: function handleLogin(user) {\n      var _this = this;\n\n      AuthService.login(user).then(function () {\n        _this.$router.push({\n          path: '/'\n        });\n      })[\"catch\"](function () {\n        _this.message = 'could not login';\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAsCA,SAASA,IAAT,EAAeC,KAAf,EAAsBC,YAAtB,QAA0C,cAA1C;AACA,OAAO,KAAKC,GAAZ,MAAqB,KAArB;AACA,OAAOC,WAAP,MAAwB,2BAAxB;AACA,eAAe;EACbC,IAAI,EAAE,WADO;EAEbC,UAAU,EAAE;IACVN,IAAI,EAAJA,IADU;IAEVC,KAAK,EAALA,KAFU;IAGVC,YAAW,EAAXA;EAHU,CAFC;EAObK,IAPa,kBAON;IACL,IAAMC,MAAK,GAAIL,GAAG,CAACM,MAAJ,GAAaC,KAAb,CAAmB;MAChCC,QAAQ,EAAER,GAAG,CAACS,MAAJ,GAAaC,QAAb,CAAsB,wBAAtB,CADsB;MAEhCC,QAAQ,EAAEX,GAAG,CAACS,MAAJ,GAAaC,QAAb,CAAsB,uBAAtB;IAFsB,CAAnB,CAAf;IAIA,OAAO;MACLE,OAAO,EAAE,KADJ;MAELC,OAAO,EAAE,EAFJ;MAGLR,MAAK,EAALA;IAHK,CAAP;EAKD,CAjBY;EAkBbS,OAAO,EAAE;IACPC,WADO,uBACKC,IADL,EACW;MAAA;;MAChBf,WAAW,CAACgB,KAAZ,CAAkBD,IAAlB,EACGE,IADH,CACQ,YAAM;QACV,KAAI,CAACC,OAAL,CAAaC,IAAb,CAAkB;UAAEC,IAAI,EAAE;QAAR,CAAlB;MACD,CAHH,WAIS,YAAM;QACX,KAAI,CAACR,OAAL,GAAe,iBAAf;MACD,CANH;IAOF;EATO;AAlBI,CAAf","names":["Form","Field","ErrorMessage","yup","AuthService","name","components","data","schema","object","shape","username","string","required","password","loading","message","methods","handleLogin","user","login","then","$router","push","path"],"sourceRoot":"","sources":["/Users/tanachok/Desktop/conBo/Project2/SE331-2022-Project-front-end/src/views/LoginFormView.vue"],"sourcesContent":["<template>\n  <div class=\"col-md-12\">\n    <div class=\"card card-container\">\n      <img\n        id=\"profile-img\"\n        src=\"//ssl.gstatic.com/accounts/ui/avatar_2x.png\"\n        class=\"profile-img-card\"\n      />\n      <Form @submit=\"handleLogin\" :validation-schema=\"schema\">\n        <div class=\"form-group\">\n          <label for=\"username\">Username</label>\n          <Field name=\"username\" type=\"text\" class=\"form-control\" />\n          <ErrorMessage name=\"username\" class=\"error-feedback\" />\n        </div>\n        <div class=\"form-group\">\n          <label for=\"password\">Password</label>\n          <Field name=\"password\" type=\"password\" class=\"form-control\" />\n          <ErrorMessage name=\"password\" class=\"error-feedback\" />\n        </div>\n        <div class=\"form-group\">\n          <button class=\"btn btn-primary btn-block\" :disabled=\"loading\">\n            <span\n              v-show=\"loading\"\n              class=\"spinner-border spinner-border-sm\"\n            ></span>\n            <span>Login</span>\n          </button>\n        </div>\n        <div class=\"form-group\">\n          <div v-if=\"message\" class=\"alert alert-danger\" role=\"alert\">\n            {{ message }}\n          </div>\n        </div>\n      </Form>\n    </div>\n  </div>\n</template>\n<script>\nimport { Form, Field, ErrorMessage } from 'vee-validate'\nimport * as yup from 'yup'\nimport AuthService from '@/services/AuthService.js'\nexport default {\n  name: 'LoginView',\n  components: {\n    Form,\n    Field,\n    ErrorMessage\n  },\n  data() {\n    const schema = yup.object().shape({\n      username: yup.string().required('User name is required!'),\n      password: yup.string().required('Password is required!')\n    })\n    return {\n      loading: false,\n      message: '',\n      schema\n    }\n  },\n  methods: {\n    handleLogin(user) {\n      AuthService.login(user)\n        .then(() => {\n          this.$router.push({ path: '/' })\n        })\n        .catch(() => {\n          this.message = 'could not login'\n        })\n    }\n  }\n}\n</script>\n<style scoped>\nlabel {\n  display: block;\n  margin-top: 10px;\n}\n.card-container.card {\n  max-width: 350px !important;\n  padding: 40px 40px;\n}\n.card {\n  background-color: #f7f7f7;\n  padding: 20px 25px 30px;\n  margin: 0 auto 25px;\n  margin-top: 50px;\n  -moz-border-radius: 2px;\n  -webkit-border-radius: 2px;\n  border-radius: 2px;\n  -moz-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n  -webkit-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n  box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);\n}\n.profile-img-card {\n  width: 96px;\n  height: 96px;\n  margin: 0 auto 10px;\n  display: block;\n  -moz-border-radius: 50%;\n  -webkit-border-radius: 50%;\n  border-radius: 50%;\n}\n.error-feedback {\n  color: red;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}